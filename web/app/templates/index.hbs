{{#ma-app}}
  {{#ma-header logo=(t 'app.name')}}
    {{ma-notification total=model.meta.total notifications=model titlePath='alert.category' descPath='alert.signature'}}
  {{/ma-header}}
  {{#ma-section}}
    {{#ma-sidebar}}
      {{ma-profile-menu user=user}}
      {{ma-menu class='main-menu' menu=menu}}
    {{/ma-sidebar}}
    {{#ma-content}}
      {{outlet}}
    {{/ma-content}}
  {{/ma-section}}
  {{ma-footer copyright=(t 'app.copyright')}}
{{/ma-app}}
