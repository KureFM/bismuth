{{#ma-card title=(t 'labels.ssh-records') padding=false}}
  <table class="table table-striped vertical-meddle">
    <thead>
      <tr>
        <th>{{t 'labels.no'}}</th>
        <th>{{t 'labels.ids.src'}}</th>
        <th>{{t 'labels.ids.desc'}}</th>
        <th>{{t 'labels.ssh-records.client.ssh-version'}}</th>
        <th>{{t 'labels.ssh-records.server.ssh-version'}}</th>
        <th>{{t 'labels.ids.timestamp'}}</th>
      </tr>
    </thead>
    <tbody>
      {{#each model as |ssh index|}}
        <tr>
          <td>{{list-item index page limit}}</td>
          <td>{{ssh.srcIp}}:{{ssh.srcPort}}</td>
          <td>{{ssh.destIp}}:{{ssh.destPort}}</td>
          <td>{{ssh.ssh.client.software_version}} / {{ssh.ssh.client.proto_version}}</td>
          <td>{{ssh.ssh.server.software_version}} / {{ssh.ssh.server.proto_version}}</td>
          <td>{{moment-format ssh.timestamp 'LLL'}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>
  <div class="bootgrid-footer">
    <div class="row">
      <div class="col-sm-6">
        {{ma-pagination total=totalPages start=startPage current=page size=windowSize}}
      </div>
    </div>
  </div>
{{/ma-card}}
